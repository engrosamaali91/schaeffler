<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >


    
    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>
    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>
    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <!-- Base Link-->
    <link name="base_link">
    </link>

    <!-- Chassis Link or Main Body-->
    <link name="chassis">
        <visual>
            <origin rpy="1.5707963267949 0 1.5707963267949" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://emma_visualization/meshes/emma_cover.stl"/>
            </geometry>
            <material name="white"/>
        </visual>
        <collision>
            <origin rpy="1.5707963267949 0 1.5707963267949" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://emma_visualization/meshes/emma_cover.stl"/>

            </geometry>
        </collision>
        <inertial>
            <mass value="4.07"/>
            <inertia ixx="0.426" ixy="0.0" ixz="0.0" iyy="0.496" iyz="0.0" izz="0.91"/>
        </inertial>
    </link>


    <!-- Chassis Joint-->
    <joint name="chassis_joint" type="fixed">

        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="0.0 0.0 0.085"/>
        
    </joint>


    <link name="left_wheel">
        <visual>
        <origin xyz="0 0 0" rpy="1.5707963267949 0 0" />
        <geometry>
            <cylinder radius="0.088" length="0.03937"/>
        </geometry>
        <material name="black"/>
        </visual>

        <collision>
        <origin xyz="0 0 0" rpy="1.5707963267949 0 0"/>
        <geometry>
            <cylinder radius="0.088" length="0.03937"/>
        </geometry>
        </collision>
        
        <inertial>
        <mass value="0.17"/>
        <inertia ixx="0.000204" ixy="0.0" ixz="0.0" iyy="0.000306" iyz="0.0" izz="0.000204"/>
        </inertial>
    </link>
    <joint name="left_wheel_joint" type="continuous">
            <parent link="chassis"/>
            <child link="left_wheel"/>
            <origin xyz="0 0.196425 0.000881" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
    </joint>


    <link name="right_wheel">
        <visual>
        <origin xyz="0 0 0" rpy="1.5707963267949 0 0" />
        <geometry>
            <cylinder radius="0.088" length="0.03937"/>
        </geometry>
        <material name="black"/>
        </visual>

        <collision>
        <origin xyz="0 0 0" rpy="1.5707963267949 0 0"/>
        <geometry>
            <cylinder radius="0.088" length="0.03937"/>
        </geometry>
        </collision>
        
        <inertial>
        <mass value="0.17"/>
        <inertia ixx="0.000204" ixy="0.0" ixz="0.0" iyy="0.000306" iyz="0.0" izz="0.000204"/>
        </inertial>
    </link>
    <joint name="right_wheel_joint" type="continuous">
            <parent link="chassis"/>
            <child link="right_wheel"/>
            <origin xyz="0 -0.196425 0.000881" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
    </joint>


    <link name="right_front_small_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707963267949 0 0"/>
            <geometry>
                <cylinder length="0.02775" radius="0.0375"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.5707963267949 0 0"/>
            <geometry>
                <cylinder length="0.02775" radius="0.0375"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.17"/>
            <inertia ixx="0.000204" ixy="0.0" ixz="0.0" iyy="0.000306" iyz="0.0" izz="0.000204"/>
        </inertial>
    </link>
    <joint name="right_front_small_wheel_joint" type="continuous">
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <parent link="chassis"/>
        <child link="right_front_small_wheel"/>
        <origin xyz="0.209 -0.108 -0.049" rpy="0 0 0"/>
    </joint>


    <link name="left_front_small_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707963267949 0 0"/>
            <geometry>
                <cylinder length="0.02775" radius="0.0375"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.5707963267949 0 0"/>
            <geometry>
                <cylinder length="0.02775" radius="0.0375"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.17"/>
            <inertia ixx="0.000204" ixy="0.0" ixz="0.0" iyy="0.000306" iyz="0.0" izz="0.000204"/>
        </inertial>
    </link>
    <joint name="left_front_small_wheel_joint" type="continuous">
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <parent link="chassis"/>
        <child link="left_front_small_wheel"/>
        <origin  xyz="0.209 0.108 -0.049" rpy="0 0 0"/>
    </joint>


    <link name="left_back_small_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707963267949 0 0" />
            <geometry>
                <cylinder length="0.02775" radius="0.0375"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.5707963267949 0 0" />
            <geometry>
                <cylinder length="0.02775" radius="0.0375"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.17"/>
            <inertia ixx="0.000204" ixy="0.0" ixz="0.0" iyy="0.000306" iyz="0.0" izz="0.000204"/>
        </inertial>
    </link>
    <joint name="left_back_small_wheel_joint" type="continuous">
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <parent link="chassis"/>
        <child link="left_back_small_wheel"/>
        <origin xyz="-0.228 0.156 -0.049" rpy="0 0 0" />
    </joint>
 


    <link name="right_back_small_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707963267949 0 0" />
            <geometry>
                <cylinder length="0.02775" radius="0.0375"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.5707963267949 0 0" />
            <geometry>
                <cylinder length="0.02775" radius="0.0375"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.17"/>
            <inertia ixx="0.000204" ixy="0.0" ixz="0.0" iyy="0.000306" iyz="0.0" izz="0.000204"/>
        </inertial>
    </link>
    <joint name="right_back_small_wheel_joint" type="continuous">
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <parent link="chassis"/>
        <child link="right_back_small_wheel"/>
        <origin xyz="-0.228 -0.156 -0.049" rpy="0 0 0" />
    </joint>

